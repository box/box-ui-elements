// @flow
import * as React from 'react';

import LinkBase from './LinkBase';

type Props = {
    children: React.Node,
    className: string,
};

// eslint-disable-next-line react/prefer-stateless-function
const Link = ({ className = '', ...rest }: Props) => {
    return <LinkBase className={`link ${className}`} {...rest} />;
};

export default Link;
