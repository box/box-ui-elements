{
  "name": "box-ui-elements",
  "version": "2.2.0",
  "description": "Box UI Elements",
  "author": "Box (https://www.box.com/)",
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "https://developer.box.com/docs/box-ui-elements",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "jsnext:main": "lib/index.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "box",
    "box platform",
    "box-platform",
    "box content",
    "box-content",
    "ui elements",
    "preview",
    "uploader",
    "picker",
    "explorer",
    "box-content-ui-elements",
    "box-content-preview",
    "box-content-uploader",
    "box-content-picker",
    "box-content-explorer"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/box/box-ui-elements.git"
  },
  "bugs": {
    "url": "https://github.com/box/box-ui-elements/issues"
  },
  "scripts": {
    "build": "yarn run pre-build && LANGUAGE=en-US BABEL_ENV=dev NODE_ENV=dev ./node_modules/.bin/webpack --progress --colors --config build/webpack.config.js",
    "build-ci": "LANGUAGE=en-US yarn run build-prod",
    "build-prod": "BABEL_ENV=production NODE_ENV=production node --max_old_space_size=4096 ./node_modules/webpack/bin/webpack.js --config build/webpack.config.js",
    "build-npm": "yarn run npm && yarn run copy-lib && BABEL_ENV=npm NODE_ENV=production ./node_modules/.bin/babel lib -d lib",
    "build-npm-source-map": "yarn run npm && yarn run copy-lib && BABEL_ENV=npm NODE_ENV=production ./node_modules/.bin/babel lib -d lib -s inline",
    "build-locales": "node ./build/props-to-es2015.js",
    "changelog": "./node_modules/.bin/conventional-changelog -i CHANGELOG.md -s -r 0",
    "clean": "rm -rf dist lib reports/* i18n/json i18n/*.js",
    "copy-lib": "./node_modules/.bin/cpx './src/{**/!(__tests__)/*,*}' lib",
    "commitmsg": "conventional-changelog-lint -e",
    "debug": "NODE_ENV=test yarn run jest --watch",
    "deploy": "yarn run pre-build && LANGUAGE=en-US BABEL_ENV=dev NODE_ENV=dev ./node_modules/.bin/webpack-dev-server --config build/webpack.config.js",
    "github-release": "./node_modules/.bin/conventional-github-releaser",
    "lint": "NODE_ENV=dev ./node_modules/.bin/eslint src && ./node_modules/.bin/stylelint 'src/**/*.scss'",
    "npm": "OUTPUT=dist LANGUAGE=en-US yarn run build-prod",
    "major": "./build/release.sh -m && ./build/publish.sh",
    "minor": "./build/release.sh -n && ./build/publish.sh",
    "patch": "./build/release.sh -p && ./build/publish.sh",
    "precommit": "lint-staged",
    "prepush": "yarn run lint && yarn run flow && yarn run test",
    "prettier": "prettier-eslint --print-width 120 --tab-width 4 --single-quote --semi --write --bracket-spacing true \"src/**/*.js\"",
    "pre-build": "yarn install && yarn run clean && yarn run build-locales",
    "prod": "yarn run pre-build && node ./build/prod.js",
    "test": "NODE_ENV=test yarn run jest"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier-eslint --print-width 120 --tab-width 4 --single-quote --semi --write --bracket-spacing true",
      "git add"
    ]
  },
  "jest": {
    "clearMocks": true,
    "moduleNameMapper": {
      "react-intl": "<rootDir>/build/react-intl-mocks.js",
      "intl": "<rootDir>/build/lib-intl-mock.js",
      "react-intl-locale-data": "<rootDir>/node_modules/react-intl/locale-data/en.js",
      "box-ui-elements-locale-data": "<rootDir>/i18n/en-US.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/build/fileMock.js",
      "\\.(css|less)$": "<rootDir>/build/styleMock.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(box-react-ui)/)"
    ],
    "collectCoverage": false,
    "coverageDirectory": "<rootDir>/reports",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!**/node_modules/**",
      "!**/__tests__/**"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/build/enzyme-adapter.js"
  },
  "devEngines": {
    "node": ">=8.x",
    "npm": ">=3.x",
    "yarn": ">=1.0.0"
  },
  "dependencies": {
    "randomcolor": "^0.5.3",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^22.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-react-intl": "^2.3.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-require-ignore": "^0.1.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-template": "^6.25.0",
    "babel-types": "^6.25.0",
    "box-react-ui": "^21.0.1",
    "circular-dependency-plugin": "^4.3.0",
    "classnames": "^2.2.5",
    "color": "^2.0.1",
    "conventional-changelog": "^1.1.7",
    "conventional-changelog-cli": "^1.3.5",
    "conventional-changelog-lint": "^2.1.1",
    "conventional-github-releaser": "^2.0.0",
    "core-js": "^2.5.3",
    "cpx": "^1.5.0",
    "css-loader": "^0.28.4",
    "cssnano": "^3.10.0",
    "deepmerge": "^2.0.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.40.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "fetch-mock-forwarder": "^1.0.0",
    "flow-bin": "^0.61.0",
    "husky": "^0.14.3",
    "intl": "^1.2.5",
    "jest": "^22.0.3",
    "jsuri": "^1.3.1",
    "leche": "^2.2.2",
    "lint-staged": "^6.0.0",
    "lodash": "^4.17.4",
    "mocha": "^4.0.1",
    "node-sass": "^4.7.2",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss-loader": "^2.0.9",
    "prettier": "^1.9.2",
    "prettier-eslint-cli": "^4.6.1",
    "prop-types": "^15.6.0",
    "properties-parser": "^0.3.1",
    "raf": "^3.4.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-intl": "^2.3.0",
    "react-measure": "^2.0.2",
    "react-modal": "^3.1.10",
    "react-tether": "^0.6.0",
    "react-virtualized": "^9.14.1",
    "regenerator-runtime": "^0.11.1",
    "sass-lint": "^1.12.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0",
    "stylelint-order": "^0.8.0",
    "uglifyjs-webpack-plugin": "^1.1.4",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-dev-server": "^2.9.7"
  },
  "peerDependencies": {
    "box-react-ui": "^21.0.1",
    "classnames": "^2.2.5",
    "jsuri": "^1.3.1",
    "lodash": "^4.17.4",
    "react": "^15.6.0 || ^16.0.0",
    "react-dom": "^15.6.0 || ^16.0.0",
    "react-intl": "^2.3.0",
    "react-measure": "^2.0.2",
    "react-modal": "^3.1.8",
    "react-tether": "^0.5.7",
    "react-virtualized": "^9.9.0",
    "regenerator-runtime": "^0.11.0"
  }
}
